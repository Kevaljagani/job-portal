<section class="flex justify-center text-4xl mt-10"><h1>Admin DashBoard</h1></section>

<section class="flex justify-center mt-24">

        <table class="table-auto">
          <thead>
            <tr>
              <th class="px-4 py-2">Sr No</th>
              <th class="px-4 py-2">id</th>
              <th class="px-4 py-2">Name</th>
              <th class="px-4 py-2">Email</th>
              <th class="px-4 py-2">Contact No</th>
              <th class="px-4 py-2">Role</th>
              <th class="px-4 py-2">Edit</th>
              <th class="px-4 py-2">Delete</th>
            </tr>
          </thead>
          <% for(var i=0; i<foo.length; i++) {%>
          <tbody>
            <tr class="bg-blue-100">
              <td name="a" class="border px-4 py-2"><%= i %></td>
              <td name="a" class="border px-4 py-2"><%= foo[i].id %></td>
              <td name="b" class="border px-4 py-2"><%= foo[i].name %></td>
              <td name="c" class="border px-4 py-2"><%= foo[i].email %></td>
              <td name="d" class="border px-4 py-2"><%= foo[i].number %></td>
              <td name="e" class="border px-4 py-2"><%= foo[i].role %></td>
              <td name="f" class="border px-4 py-2"><form action="/edit" method="GET"><button class="bg-green-500 hover:bg-blue-700 text-white font-bold py-1 px-3" type="submit">Edit</form></td>
              	<td class="border px-4 py-2"><form action="/edit" method="GET"><button class="bg-red-500 hover:bg-blue-700 text-white font-bold py-1 px-3" type="submit">Delete</form></td>
            </tr>
          </tbody>
          <% } %>
        </table>
  </section>
